{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
      "name": "operations-api",
      "urls": [
        "https://api-base.herokuapp.com/api/priv/monthbalances",
        "https://api-base.herokuapp.com/api/priv/journalentries"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "5s"
      }
    },
    {
      "name": "operations-api",
      "urls": [
        "https://api-base.herokuapp.com/api/pub/categories/months",
        "https://api-base.herokuapp.com/api/pub/categories/entry_kinds",
        "https://api-base.herokuapp.com/api/pub/categories/expense_categories"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1h"
      }
    }
  ]
}